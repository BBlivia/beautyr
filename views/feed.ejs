<%- include('partials/header') -%>
 

<section class="px-4 py-24 mx-auto max-w-7xl">
  <h1 class="mb-2 text-3xl font-extrabold leading-tight text-gray-900">Explore Reviews</h1>


  <div class="grid grid-cols-1 gap-12 md:grid-cols-2 lg:grid-cols-2 xl:grid-cols-3">
  <% for(var i=0; i<posts.length; i++) {%>
  <div>
    
      
      <a>
        <img src="<%= posts[i].image%>" class="object-cover w-full h-56 mb-5 bg-center rounded" alt="...">
      </a>
     
     
        <p class="mb-2 text-lg font-semibold text-gray-900"> Title: <%= posts[i].serviceProvider%> </p>
        <h5 class="card-title">IG Name: <%= posts[i].title%></h5>
        <button type="submit" class="bg-white">
          <svg aria-label="Like" class="_ab6-" color="black"   height="12" role="img" viewBox="0 0 24 24" width="12"><path d="M16.792 3.904A4.989 4.989 0 0 1 21.5 9.122c0 3.072-2.652 4.959-5.197 7.222-2.512 2.243-3.865 3.469-4.303 3.752-.477-.309-2.143-1.823-4.303-3.752C5.141 14.072 2.5 12.167 2.5 9.122a4.989 4.989 0 0 1 4.708-5.218 4.21 4.21 0 0 1 3.675 1.941c.84 1.175.98 1.763 1.12 1.763s.278-.588 1.11-1.766a4.17 4.17 0 0 1 3.679-1.938m0-2a6.04 6.04 0 0 0-4.797 2.127 6.052 6.052 0 0 0-4.787-2.127A6.985 6.985 0 0 0 .5 9.122c0 3.61 2.55 5.827 5.015 7.97.283.246.569.494.853.747l1.027.918a44.998 44.998 0 0 0 3.518 3.018 2 2 0 0 0 2.174 0 45.263 45.263 0 0 0 3.626-3.115l.922-.824c.293-.26.59-.519.885-.774 2.334-2.025 4.98-4.32 4.98-7.94a6.985 6.985 0 0 0-6.708-7.218Z"></path>
          </svg>
        </button> 
        <span class="inline"> <%= posts[i].likes %></span>
        
        <p class="card-text"><small class="text-muted">By: <%= userName[i]%></small></p>
        <label for="caption" class="form-label font-bold"> experience</label>
        <p class="card-text"><%= posts[i].caption %></p>
        <a href="/post/<%= posts[i]._id%>" class="btn btn-outline-primary">View Review</a>
  
      <div class="card-footer">
        <span class="badge bg-blue"><%= posts[i].location%></span>
        

       
      </div>
  </div>
  <% } %>
</div>
</section>

    
    
  
